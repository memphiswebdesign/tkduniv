<script src="https://player.vimeo.com/api/player.js"></script>

<script>
	document.addEventListener("DOMContentLoaded", () => {

		// hero vid load class
	    const iframe = document.querySelector("#landing-video iframe");
	    const landingHero = document.querySelector("#landing-hero");

	    if (iframe) {
	        const player = new Vimeo.Player(iframe);

	        // Listen for when the video starts playing
	        player.on("play", () => {
	            landingHero.classList.add("vid-load");
	        });
	    }

	    // Customer Spark + Netlify form
	    (function () {
		  const SPARK_ENDPOINT = "https://app.sparkmembership.com/wf/process.aspx";
		  const form = document.getElementById("tkd-lead-form");

		  if (!form) return;

		  form.addEventListener("submit", async function (e) {
		    // Prevent infinite loops if we call form.submit()
		    if (form.dataset.sparkSubmitted === "true") return;

		    e.preventDefault();

		    // Build payload from the form fields (includes Spark hidden inputs)
		    const fd = new FormData(form);

		    // Spark expects standard form post encoding
		    const body = new URLSearchParams();
		    for (const [k, v] of fd.entries()) body.append(k, v);

		    try {
		      // Spark may not send CORS headers. no-cors allows the POST to still be sent.
		      await fetch(SPARK_ENDPOINT, {
		        method: "POST",
		        mode: "no-cors",
		        headers: { "Content-Type": "application/x-www-form-urlencoded" },
		        body: body.toString(),
		      });
		    } catch (err) {
		      // If Spark is down, still submit to Netlify so you don't lose the lead.
		      console.warn("Spark submission failed; continuing with Netlify submit.", err);
		    }

		    // Now submit to Netlify normally
		    form.dataset.sparkSubmitted = "true";
		    form.submit();
		  });
		})();

</script>

